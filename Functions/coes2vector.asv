function [r, v, mu] = coes2vector(coe)
    % Harvey
    % Converts COEs to r v vector
    %coe = [h, e, RA, inc, w, TA, a];
    h = coe(1);
    e = coe(2);
    RAAN = coe(3);
    inc = coe(4);
    w = coe(5);
    TA = coe(6);
    a = coe(7);

    % Perifocal frame
    r = h^2/(mu*(1 + e*cos(TA)))*[cos(TA);sin(TA);0];
    v = mu/h*[-sin(TA);e + cos(TA), 0];

    CECItoPERI = Cz(w)*Cx(inc)*Cz(RAAN);
    CPERItoECI = CECItoPERI';

    r
end